<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
</head>
<body>
    <table border="1">
        <thead>
            <tr>
                <th>STT</th>
                <th>Name</th>
                <th>Age</th>
                <th>Address</th>
                <th>Phone</th>
                <th>Email</th>
                <th>Avatar</th>
            </tr>
        </thead>
        <tbody>
            <% let stt = 1 %>
            <% staffArray.forEach(staff=> { %>
                    <tr>
                        <td><%= stt %></td>
                        <td><%= staff.name %></td>
                        <td><%= staff.age %></td>
                        <td><%= staff.address %></td>
                        <td><%= staff.phone %></td>
                        <td><%= staff.email %></td>
                        <td><%= staff.avatar %></td>
                    </tr>
            <% stt++ %>
            <% }) %>
        </tbody>
    </table>
</body>
</html>